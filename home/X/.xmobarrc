Config { font            = "xft:Iosevka:size=12:antialias=true:hinting=true"
       , additionalFonts = []
       , bgColor         = "#5F5F5F"
       , fgColor         = "#ECF0F1"
       , position        = TopW L 100
       , lowerOnStart    = True
       , sepChar         = "%"
       , alignSep        = "}{"
       , template        = "%UnsafeStdinReader%}{%cpu%%memory%%disku%<action=`termite -e nmtui`>%wlan0wi%%enp0s3%</action>%default:Master%%battery%%date%"
       , commands = [ Run Cpu              [ "-t", " CPU <total>% "
                                           , "-L", "3", "-H", "50"
                                           , "-n", "#2ECC71"
                                           , "-h", "#E74C3C" ] 10
                    , Run Memory           [ "-t", "\xE0B3 MEM <usedratio>% " ] 10
                    , Run DiskU            [ ("/", "\xE0B3 DRV <usedp>% ") ] [] 100
                    , Run Network "enp0s3" [ "-t", "<fc=#EEEEEE,#878787><rx>\x2193\x2191<tx> </fc>" ] 10
                    , Run Wireless "wlan0" [ "-t", "<fc=#878787,#5F5F5F>\xE0B2</fc><fc=#EEEEEE,#878787> WIFI <quality>% </fc>"] 10
                    , Run Battery          [ "-t", "<fc=#EEEEEE,#878787>\xE0B3 <acstatus> <left>% </fc>"
                                           , "--"
                                           , "-O", "AC"
                                           , "-i", "AC"
                                           , "-o", "BAT"
                                           , "-h", "#2ECC71"
                                           , "-l", "#E74C3C" ] 10
                    , Run Volume "default" "Master"
                                           [ "-t", "<fc=#EEEEEE,#878787>\xE0B3 VOL <volume>% </fc>" ] 10
                    , Run Date "<fc=#2ECC71,#878787>\xE0B2</fc><fc=#5F5F5F,#2ECC71> %a \xE0B3 %H:%M \xE0B3 %d.%m.%Y </fc>" "date" 10
                    , Run UnsafeStdinReader
                    ]
       }
