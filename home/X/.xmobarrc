Config { font            = "xft:Iosevka:size=12:antialias=true:hinting=true"
       , additionalFonts = []
       , bgColor         = "#5F5F5F"
       , fgColor         = "#ECF0F1"
       , position        = TopW L 100
       , lowerOnStart    = True
       , sepChar         = "%"
       , alignSep        = "}{"
       , template        = "%UnsafeStdinReader%}{%cpu%%memory%%disku%%default:Master%%dynnetwork%%battery%%date%"
       , commands = [ Run Cpu                       [ "-t", " cpu <vbar> "
                                                    , "-L", "30", "-H", "70"
                                                    , "-n", "#F1C40F", "-h", "#E74C3C"
                                                    ] 10
                    , Run Memory                    [ "-t", "\xE0B3 mem <usedvbar> "
                                                    , "-L", "30", "-H", "70"
                                                    , "-n", "#F1C40F", "-h", "#E74C3C"
                                                    ] 10
                    , Run DiskU                     [ ("/", "\xE0B3 drv <usedvbar> ") ]
                                                    [ "-L", "30", "-H", "70"
                                                    , "-n", "#F1C40F", "-h", "#E74C3C"
                                                    ] 100
                    , Run Battery                   [ "-t", "<fc=#EEEEEE,#878787>\xE0B3 <acstatus> <leftvbar> </fc>"
                                                    , "--"
                                                    , "-O", "ac"
                                                    , "-i", "ac"
                                                    , "-o", "bat"
                                                    , "-L", "30", "-H", "70"
                                                    , "-m", "#F1C40F", "-l", "#E74C3C"
                                                    ] 10
                    , Run Volume "default" "Master" [ "-t", "<fc=#878787,#5F5F5F>\xE0B2</fc><fc=#EEEEEE,#878787> vol <volumevbar> </fc>" ] 10
                    , Run DynNetwork                [ "-t", "<fc=#EEEEEE,#878787>\xE0B3  <dev> <rx>↓↑<tx> </fc>", "--", "--devices", "enp0s3"] 10
                    , Run Date "<fc=#2ECC71,#878787>\xE0B2</fc><fc=#5F5F5F,#2ECC71> %a \xE0B3 %H:%M \xE0B3 %d.%m.%Y </fc>" "date" 10
                    , Run UnsafeStdinReader
                    ]
       }
