Config { font = "xft:Iosevka Term:size=10:antialias=true"
       , additionalFonts = []
       , bgColor = "#2C3E50"
       , fgColor = "#ECF0F1"
       , position = TopW L 100
       , lowerOnStart = True
       , commands = [ Run Cpu ["-L","3","-H","50","--normal","#2ECC71","--high","#E74C3C"] 10
                    , Run Memory ["-t","Mem: %"] 10
                    , Run Swap [] 10
                    , Run Date "%a %Y.%m.%d %H:%M" "date" 10
                    , Run UnsafeStdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = " %UnsafeStdinReader% }{ %cpu% | %memory% * %swap% | %date% "
       }
