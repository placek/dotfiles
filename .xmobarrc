Config { font = "xft:Iosevka Term:size=12:antialias=true"
       , additionalFonts = []
       , bgColor = "#2C3E50"
       , fgColor = "#ECF0F1"
       , position = TopW L 100
       , lowerOnStart = True
       , commands = [ Run Cpu                 [ "-t", "<fc=#F1C40F>CPU <total>%</fc>" ] 10
                    , Run Memory              [ "-t", "<fc=#9B59B6>MEM <usedratio>%</fc>" ] 10
                    , Run BatteryP [ "BAT0" ] [ "-t", "<fc=#2ECC71>BAT <left>%</fc>" ] 600
                    , Run Wireless   "wlan0"  [ "-t", "<fc=#3498DB>WIFI <quality>%</fc>"] 600
                    , Run Date       "%a %Y.%m.%d %H:%M" "date" 10
                    , Run UnsafeStdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = " %UnsafeStdinReader% }{ %cpu% %memory% %wiwlan0% %battery% <action=xdotool key alt+shift+l>%date%</action> "
       }
