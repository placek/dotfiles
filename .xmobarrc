Config { font = "xft:Iosevka Term:size=10:antialias=true"
       , additionalFonts = []
       , bgColor = "#2C3E50"
       , fgColor = "#ECF0F1"
       , position = TopW L 100
       , lowerOnStart = True
       , commands = [ Run Cpu ["-t","CPU <total>%"] 10
                    , Run Memory ["-t","MEM <usedratio>%"] 10
                    , Run BatteryP ["BAT0"] ["-t","BAT <left>%"] 600
                    , Run Wireless "wlan0" ["t","WIFI <quality>%"] 600
                    , Run Date "%a %Y.%m.%d %H:%M" "date" 10
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = " %StdinReader% }{ %cpu% | %memory% | %wiwlan0% | %battery% | %date% "
       }
